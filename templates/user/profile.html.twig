{% extends 'base.html.twig' %}

{% block body %}
    <form method="post" enctype="multipart/form-data">
        {{ form_start(form) }}
        {{ form_row(form.email) }}
        {{ form_row(form.firstName) }}
        {{ form_row(form.lastName) }}
        {{ form_row(form.image) }}
        <button class="btn btn-primary">Save</button>
        {{ form_end(form) }}
    </form>

{% endblock %}

{% block title %}

{% endblock %}

